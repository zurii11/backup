! special colors
*.foreground:  #ebf1db
*.background:  #29262d
*.cursorColor:  #9ea7a6
! black
*.color0:      #29262d
*.color8:      #3f4944
! red
*.color1:      #2a5491
*.color9:      #2a5491
! green
*.color2:      #237986
*.color10:     #237986
! yellow
*.color3:      #a03b1e
*.color11:     #a03b1e
! blue
*.color4:      #484d79
*.color12:     #484d79
! magenta
*.color5:      #c59820
*.color13:     #c59820
! cyan
*.color6:      #b02f30
*.color14:     #b02f30
! white
*.color7:      #9ea7a6
*.color15:     #ebf1db
! underlined text
*.colorUL:     #178CE5
! highlighted text color
*.highlightColor: #252935
#.highlightTextColor: #C0C5CE

URxvt*scrollBar:         false

URxvt.scrollTtyKeypress: true
URxvt.scrollTtyoutput: false
URxvt.scrollWithBuffer: false
URxvt.fading: 30
URxvt.termName : rxvt-unicode
URxvt.iconFile: ~/terminal_icon.svg
URxvt.geometry: 64x22


URxvt.font: xft:monospace:pixelsize=16
URxvt.letterSpace: 0

URxvt.perl-ext-common: default,matcher,tabbed,clipboard,resize-font
URxvt.url-launcher: firefox
URxvt.matcher.button: 1

URxvt.tabbed.tabbar-fg: 14
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 15
URxvt.tabbed.tab-bg: 8

URxvt.clipboard.autocopy: false
URxvt.keysym.C-c: perl:clipboard:copy
URxvt.keysym.C-v: perl:clipboard:paste

URxvt.keysym.C-Up:    resize-font:bigger
URxvt.keysym.C-Down:  resize-font:smaller
